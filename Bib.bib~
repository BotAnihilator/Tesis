@article{TUP,
	Abstract = {At high energies, beam-induced synchrotron radiation is an important source of heating, beam-related vacuum pressure increase and primary photo-electrons which can give rise to an electron cloud. The photon distribution along the beam pipe wall is a key input to codes as ECLOUD or PyECLOUD, codes which model the electron cloud build-up. For future high-energy colliders, like TLEP or SHE-LHC, photon stops and antechambers are considered in order to facilitate
cooling and vacuum pressure control. We use the synrad3d code developed at Cornell to simulate the distribution for the LHC and compare the result with the cos^2 or cos^3 distributions hitherto employed for LHC electron-cloud studies.
The distribution for the possible future colliders beyond the LHC, like TLEP and SHE-LHC, are also investigated to provide input to the vacuum-system design.},
	Author = {H. Maury and D. Sagan and F. Zimmermann},
	Journal = {Proceedings of IPAC2013, Shanghai, China},
	Month = {December},
	Numpages = {3},
	Pages = {1-3},
	Title = {Synchrotron-radiation photon distributions for highest-energy circular colliders},
	Url = {http://link.aip.org/link/?RSI/72/4477/1},
	Year = {2013}}
